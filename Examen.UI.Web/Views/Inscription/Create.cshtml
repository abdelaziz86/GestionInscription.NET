@model Examen.ApplicationCore.Domain.Inscription

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Inscription</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="dateInscription" class="control-label"></label>
                <input asp-for="dateInscription" class="form-control" />
                <span asp-validation-for="dateInscription" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="tauxReduction" class="control-label"></label>
                <input asp-for="tauxReduction" class="form-control" />
                <span asp-validation-for="tauxReduction" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Seminaire" class="control-label"></label>
                <select asp-for="SeminiareFK" class="form-control" asp-items="ViewBag.Seminaires">
                    
                </select>
                <span asp-validation-for="SeminiareFK" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Participant" class="control-label"></label>
                <select asp-for="ParticipantFK" class="form-control" asp-items="ViewBag.Participants">
                     
                </select>
                <span asp-validation-for="ParticipantFK" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

